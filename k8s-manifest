---
apiVersion: apps/v1
kind: Deployment
metadata:
  name: Dev-bioMedical-deploy
spec:
  selector:
    matchLabels:
      app: Dev-bioMedical
  replicas: 2 
  template:
    metadata:
      labels:
        app: Dev-bioMedical
    spec:
      containers:
      - name: bioMedical
        image: 381492147345.dkr.ecr.us-east-1.amazonaws.com/biomedical/images:72
        ports:
        - containerPort: 8082

---
apiVersion: v1
kind: Service
metadata:
  name: Dev-bioMedical-svc
spec:
  type: NodePort
  selector:	
    app: Dev-bioMedical
  ports:
    - protocol: TCP
      port: 80
      targetPort: 80
